	half = windHeight/2;

	$('#about').css("height", half);
	
	aboutHeight = $('#about').css('height');
	$('#bigger').css("height", aboutHeight);
	$('#bigpurple').css("height", aboutHeight);
	$('#blackbox').css("height", aboutHeight);
	$('#blog').css('height', aboutHeight);

});






